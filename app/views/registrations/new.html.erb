<h1>Sign Up</h1>
<%= form_with model: @user, url: registrations_path do |f| %>
  <div>
    <%= f.label :email %>
    <%= f.email_field :email %>
  </div>
  <div>
    <%= f.label :password %>
    <%= f.password_field :password %>
  </div>
  <div>
    <%= f.label :first_name %>
    <%= f.text_field :first_name %>
  </div>
  <div>
    <%= f.label :last_name %>
    <%= f.text_field :last_name %>
  </div>
  <div>
    <%= f.submit "Sign Up" %>
  </div>
  <%= link_to "Already have an account? Log in", new_session_path %>
<% end %>
