<h1>Login</h1>
<%= form_with model: @user, url: sessions_path do |f| %>
  <div>
    <%= f.label :email %>
    <%= f.email_field :email %>
  </div>
  <div>
    <%= f.label :password %>
    <%= f.password_field :password %>
  </div>
  <div>
    <%= f.submit "Login" %>
  </div>
  <%= link_to "Sign Up", new_registration_path %>
<% end %>
